{
  "manifest_version": 2,
  "name": "BigBlueButton",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "key": "<public_key>",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "oauth2": {
    "client_id": "<client_id>",
    "scopes":[
      "profile",
      "email",
      "openid"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "BigBlueButton",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}